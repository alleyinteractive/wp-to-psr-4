{
    "name": "alleyinteractive/wp-to-psr-4",
    "description": "Convert a WordPress codebase to PSR-4",
    "license": "GPL-2.0-or-later",
    "type": "project",
    "authors": [
        {
            "name": "Sean Fisher",
            "email": "srtfisher@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "symfony/console": "^5.0",
        "symfony/process": "^5.0",
        "symfony/finder": "^6.3",
        "illuminate/support": "^10.32",
        "symfony/var-dumper": "^6.3"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Alley\\WpToPsr4\\": "src/"
        }
    },
    "scripts": {
        "pint": "pint bin/wp-to-psr-4 src",
        "test": ""
    }
}
